{
  "name": "serverexpress",
  "version": "1.0.0",
  "description": "Criação de um servidor com express e implementação de middleware, gerenciamento de postagens e banco de dados",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec node src/index.js",
    "production": "cross-env NODE_ENV=production node src/index.js",
    "migrate-dev": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "migrate-prod": "cross-env NODE_ENV=production npx sequelize-cli db:migrate"
  },
  "author": "Felipe Carvalho",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.9"
  },
  "dependencies": {
    "ajv": "^8.6.1",
    "ajv-formats": "^2.1.0",
    "bcrypt": "^5.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  }
}
